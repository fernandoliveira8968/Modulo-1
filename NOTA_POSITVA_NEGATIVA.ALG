algoritmo "Nota_Positva_Negativa"
// Autor : Fernando Oliveira
// Data : 22/09/2017
// Cálculo da nota
var
   nota : inteiro
inicio
      escreva("Qual a nota? ")
      leia(nota)
      se(nota>=10) entao
      escreva("Positiva")
      senao
      Escreva("Negativa")
      fimse
fimalgoritmo
